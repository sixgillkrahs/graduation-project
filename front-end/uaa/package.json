  "type": "module",
  "scripts": {
    "dev": "vite --mode staging",
    "start": "vite --mode production",
    "build:stag": "tsc -b && vite build --mode staging",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "eslint .",
